{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "unique": false,
      "configurable": false,
      "required": false,
      "default": "cannot configure"
    },
    "email": {
      "type": "email",
      "unique": true,
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "required": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": true,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "birthdate": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "M",
        "F"
      ]
    },
    "height": {
      "type": "float",
      "min": 0
    },
    "weight": {
      "type": "float",
      "min": 0
    },
    "eatCycleList": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::eat-cycle.eat-cycle",
      "mappedBy": "user"
    },
    "waterReminder": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "exercisCycleList": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exercise-cycle.exercise-cycle",
      "mappedBy": "user"
    },
    "exerciseTypeList": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exercise-type.exercise-type",
      "mappedBy": "user"
    },
    "sleepCycleList": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sleep-cycle.sleep-cycle",
      "mappedBy": "user"
    },
    "waterCycleList": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::water-cycle.water-cycle",
      "mappedBy": "user"
    },
    "auth": {
      "type": "string",
      "default": "local"
    }
  }
}
